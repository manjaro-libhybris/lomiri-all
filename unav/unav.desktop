[Desktop Entry]
Name=uNav
Comment=Simple and easy GPS navigator
Keywords=map;maps;GPS;navigator;navigation;OSM;OpenStreetMap;
Type=Application
Icon=/usr/share/unav/icon.png
Exec=qmlscene --scaling %u /usr/share/unav/qml/Main.qml
Terminal=false
X-Ubuntu-Touch=true

X-Ayatana-Appmenu-Show-Stubs=false
X-Ubuntu-Single-Instance=true
X-Ubuntu-Splash-Show-Header=false
X-Ubuntu-Splash-Image=/usr/share/unav/icon-splash.svg
X-Ubuntu-Splash-Color-Header=#3575db
X-Ubuntu-Splash-Color=#3575db
X-Ubuntu-Splash-Color-Footer=#3575db
